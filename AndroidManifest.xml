<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alibaba.alisecuritycenter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />
	<uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
	<uses-permission android:name="android.permission.GET_TASKS" />
	<uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.alibaba.alisecuritycenter.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- MemorySpeedUp Activity start -->
        <activity
            android:name="com.alibaba.alisecuritycenter.memoryspeedup.MemorySpeedUpActivity"
            android:label="@string/app_name" 
            android:theme="@style/MyCustomTheme">
        </activity>
        <activity
            android:name="com.alibaba.alisecuritycenter.memoryspeedup.runnningprocess.RunningProgressListViewActivity"
            android:label="@string/app_name" 
            android:theme="@style/MyCustomTheme">
        </activity>
        <activity
            android:name="com.alibaba.alisecuritycenter.memoryspeedup.runnningprocess.ProcessDetailActivity"
            android:label="@string/app_name" 
            android:theme="@style/MyCustomTheme">
        </activity>
        <activity
            android:name="com.alibaba.alisecuritycenter.memoryspeedup.runningservice.RunningServiceActivity"
            android:label="@string/app_name" 
            android:theme="@style/MyCustomTheme">
        </activity>
        <activity
            android:name="com.alibaba.alisecuritycenter.memoryspeedup.whitelist.WhiteListActivity"
            android:label="@string/app_name" 
            android:theme="@style/MyCustomTheme">
        </activity>
        <!-- MemorySpeedUp Activity end -->


        <!-- PrivacySpace Activity start -->
        
        <activity
            android:name="com.alibaba.alisecuritycenter.aliprivacyspace.GestureEditActivity"
            android:theme="@android:style/Theme.NoTitleBar">
        </activity>
        
        <activity
            android:name="com.alibaba.alisecuritycenter.aliprivacyspace.GestureVerifyActivity"
            android:theme="@android:style/Theme.NoTitleBar">
        </activity>
        <activity
            android:name="com.alibaba.alisecuritycenter.aliprivacyspace.PrivacySpaceActivity"
            android:theme="@style/MyCustomTheme">
        </activity>
        
        <activity
            android:name="com.alibaba.alisecuritycenter.aliprivacyspace.communication.PrivateCommunicationActivity"
            android:theme="@style/MyCustomTheme">
        </activity>
        
        <activity
            android:name="com.alibaba.alisecuritycenter.aliprivacyspace.communication.AddPrivateContactsActivity"
            android:theme="@style/MyCustomTheme">
        </activity>
        
        <!-- PrivacySpace Activity end -->
        
    </application>
	

</manifest>
